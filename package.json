{
  "name": "service",
  "version": "0.0.0",
  "description": "Scaffolding for a serverless typescript dotenv project",
  "scripts": {
    "index": "tsc && NODE_ENV=development node dist/index.js",
    "index_win": "tsc&&set NODE_ENV=development&&node dist/index.js",
    "test": "NODE_ENV=testing jest --setupFiles dotenv/config",
    "test_coverage": "yarn test --collectCoverage",
    "lint": "eslint . --ext .ts",
    "lint_fix": "yarn lint --fix",
    "compile": "tsc --noEmit && yarn lint"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/express-serve-static-core": "^4.17.2",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.2",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.15.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "serverless": "^1.59.3",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.12.1",
    "serverless-plugin-include-dependencies": "^4.0.0",
    "ts-jest": "^24.2.0",
    "typescript": "3.7.2"
  },
  "dependencies": {
    "@aelesia/commons": "^0.0.3",
    "@aelesia/http": "^0.1.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "httyp": "^0.2.0"
  },
  "repository": ""
}
